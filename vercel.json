{
  "version": 2,
  "builds": [
    {
      "src": "packages/client/api/serverless.ts",
      "use": "@vercel/node"
    },
    {
      "src": "packages/client/vite.config.ts",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/client/api/serverless.ts"
    },
    {
      "src": "/(.*)",
      "dest": "packages/client/dist/$1"
    }
  ]
}
